{
  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "..."
      }
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "cipher": {
      "command": "cipher",
      "args": ["--mode", "mcp"],
      "env": {
        "CIPHER_WORKSPACE": "${workspaceFolder}/.cipher",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "CIPHER_EMBEDDER": "local"
      }
    }
  }
}